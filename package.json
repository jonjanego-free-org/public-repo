{
    "name": "@soluto-private/expert-workspace",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "build:dev": "turbo run build:dev",
        "build:ea": "turbo build:ea",
        "build:local": "turbo run build:local",
        "build:prod": "turbo run build:prod",
        "dev": "turbo run dev --filter=expert-ui",
        "dev:preview": "pnpm run build:local && cd apps/expert-ui && pnpm preview",
        "format": "prettier . --write --ignore-unknown",
        "lint": "turbo run lint --filter=[origin/main]",
        "lint:fix": "turbo run lint:fix --filter=[origin/main]",
        "prepare": "husky",
        "start:ea": "turbo run start:ea --filter=expert-assist",
        "start:evl": "turbo run start:evl --filter=expert-voice-link",
        "test:e2e:expert-assist": "turbo run test:e2e:local --filter=expert-assist",
        "test:e2e:expert-ui": "turbo run test:e2e:local --filter=expert-ui",
        "test:unit": "turbo run test --filter='...[origin/main]'"
    },
    "dependencies": {
        "@mantine/core": "^7.10.1",
        "@mantine/hooks": "^7.10.1",
        "@soluto-private/eventualize": "1.7.0",
        "@soluto-private/eventualize-react": "1.3.0",
        "contentful": "^10.8.4",
        "envalid": "^8.0.0",
        "immer": "^10.1.1",
        "ky": "^1.5.0",
        "pino": "^8.19.0",
        "react": "^18.2.0",
        "zustand": "4.5.5"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.1",
        "@commitlint/config-conventional": "^17.7.0",
        "@testing-library/react": "^15.0.2",
        "@types/node": "^20.12.7",
        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "@vitejs/plugin-react": "^4.2.1",
        "bundlesize": "^0.18.2",
        "dotenv-cli": "^7.4.1",
        "eslint": "^8.57.0",
        "eslint-config-custom": "workspace:*",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prefer-smart-quotes": "^2.0.0",
        "happy-dom": "^14.10.1",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.7",
        "msw": "^2.2.14",
        "prettier": "^3.3.2",
        "prettier-plugin-packagejson": "^2.5.0",
        "react-dom": "^18.2.0",
        "react-oidc-context": "^2.3.1",
        "react-router-dom": "^6.22.3",
        "tsconfig": "workspace:*",
        "turbo": "^2.0.6",
        "typescript": "^5.5.4",
        "vite": "^5.2.9",
        "vitest": "^2.0.3"
    },
    "packageManager": "pnpm@9.5.0",
    "engines": {
        "node": "^20.x",
        "pnpm": "^9.x"
    }
}
